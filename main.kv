#:kivy 2.1.0

#:include screen_main.kv
#:include screen_splash.kv

ScreenManager:
    id: screen_manager
    Screen:
        name: 'screen_splash'
        ScreenSplash:
            screen_manager: screen_manager
            app_window: app
            setting_screen: screen_main

    Screen:
        name: 'screen_main'
        ScreenMain:
            id: screen_main
